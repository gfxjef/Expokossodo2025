      "Bash(cat:*)",
      "Bash(npm run build:*)",
      "Bash(find:*)",
      "Bash(rg:*)",
      "Bash(rm:*)",
      "Bash(npm install)",
      "Bash(npm start)",
      "Bash(npx react-scripts build:*)",
      "Bash(od:*)",
      "Bash(true)",
      "Bash(npm ls:*)",
      "Bash(grep:*)",
      "Bash(CI=true npm run build)",
      "Bash(npm install:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(curl:*)",
      "Bash(python:*)",
      "Bash(npx kill-port:*)",
